{
  "name": "@apollo/graphql-testing-library",
  "version": "0.3.0",
  "private": true,
  "repository": {
    "url": "git+https://github.com/apollographql/graphql-testing-library"
  },
  "keywords": [
    "apollo",
    "apollo-client",
    "graphql",
    "msw"
  ],
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./index.cjs",
  "module": "./index.js",
  "exports": {
    "require": {
      "types": "./index.d.cts",
      "default": "./index.cjs"
    },
    "default": {
      "types": "./index.d.ts",
      "default": "./index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "clean-storybook": "rm -rf storybook-static",
    "prepdist:changesets": "node scripts/prepareDist.cjs",
    "changeset-publish": "pnpm run clean && pnpm run build && pnpm run prepdist:changesets && cd dist && changeset publish",
    "changeset-version": "changeset version && pnpm i",
    "changeset-check": "changeset status --verbose --since=origin/main",
    "generate-types": "graphql-codegen --config codegen.ts",
    "test:jest": "jest",
    "test:vitest": "vitest run",
    "test:storybook": "test-storybook",
    "relay": "relay-compiler",
    "lint": "eslint --ext .ts src",
    "prettier": "prettier --check .",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "pnpm run build",
    "build-and-test-storybook": "pnpm run build-storybook && npx concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"npx http-server storybook-static --port 6006 --silent\" \"npx wait-on tcp:6006 && pnpm run test:storybook\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "pnpm run clean-storybook && storybook build"
  },
  "license": "MIT",
  "devDependencies": {
    "@apollo/client": "3.12.4",
    "@apollo/tailwind-preset": "0.2.0",
    "@changesets/changelog-github": "0.5.0",
    "@changesets/cli": "2.27.8",
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/typescript": "4.1.2",
    "@graphql-codegen/typescript-resolvers": "4.4.1",
    "@graphql-tools/jest-transform": "2.0.0",
    "@playwright/test": "1.49.1",
    "@storybook/addon-docs": "8.4.7",
    "@storybook/addon-essentials": "8.4.7",
    "@storybook/addon-interactions": "8.4.7",
    "@storybook/addon-links": "8.4.7",
    "@storybook/blocks": "8.4.7",
    "@storybook/react": "8.4.7",
    "@storybook/react-vite": "8.4.7",
    "@storybook/test": "8.4.7",
    "@storybook/test-runner": "0.20.1",
    "@svgr/plugin-jsx": "8.1.0",
    "@svgr/plugin-svgo": "8.1.0",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.1.0",
    "@tsconfig/recommended": "1.0.7",
    "@types/jest": "29.5.13",
    "@types/node": "22.10.2",
    "@types/react": "19.0.2",
    "@types/react-dom": "19.0.2",
    "@types/react-relay": "18.2.0",
    "@types/relay-runtime": "18.2.3",
    "@typescript-eslint/eslint-plugin": "8.18.1",
    "@typescript-eslint/parser": "8.18.1",
    "babel-plugin-relay": "18.2.0",
    "concurrently": "9.1.0",
    "eslint": "8.57.0",
    "eslint-plugin-storybook": "0.11.1",
    "graphql": "16.10.0",
    "graphql-ws": "5.16.0",
    "http-server": "14.1.1",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jsdom": "25.0.1",
    "msw": "2.7.0",
    "msw-storybook-addon": "2.0.3",
    "playwright": "1.49.1",
    "postcss": "8.4.47",
    "prettier": "3.4.2",
    "react": "19.0.0",
    "react-relay": "18.2.0",
    "relay-compiler": "18.2.0",
    "relay-runtime": "18.2.0",
    "storybook": "8.4.7",
    "tailwind": "4.0.0",
    "tailwindcss": "3.4.13",
    "ts-jest": "29.2.5",
    "ts-jest-resolver": "2.0.1",
    "tsup": "8.3.0",
    "tw-colors": "3.3.2",
    "typescript": "5.7.2",
    "undici": "7.2.0",
    "vite": "6.0.5",
    "vite-plugin-graphql-loader": "4.0.4",
    "vite-plugin-relay": "2.1.0",
    "vite-plugin-svgr": "4.3.0",
    "vitest": "2.1.1",
    "wait-on": "8.0.1"
  },
  "dependencies": {
    "@bundled-es-modules/statuses": "^1.0.1",
    "@graphql-tools/executor": "^1.2.7",
    "@graphql-tools/merge": "^9.0.4",
    "@graphql-tools/mock": "^9.0.4",
    "@graphql-tools/schema": "^10.0.4",
    "@graphql-tools/utils": "^10.3.2",
    "@types/statuses": "^2.0.5",
    "graphql-tag": "^2.12.6",
    "is-node-process": "^1.2.0",
    "outvariant": "^1.4.3"
  },
  "peerDependencies": {
    "graphql": "^15.0.0 || ^16.0.0",
    "msw": "^2.0.0"
  },
  "msw": {
    "workerDirectory": [
      ".storybook/public"
    ]
  },
  "relay": {
    "src": "./.storybook/stories/components/relay",
    "schema": "./.storybook/stories/schemas/ecommerce.graphql",
    "language": "typescript",
    "eagerEsModules": true,
    "exclude": [
      "**/node_modules/**",
      "**/__mocks__/**",
      "**/__generated__/**"
    ]
  }
}
